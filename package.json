{
  "name": "wiboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run generate-html && vite build",
    "generate-html": "mkdir -p dist && cp index.html dist/",
    "lint": "eslint .",
    "preview": "vite preview",
    "postinstall": "flowbite-react patch",
    "i": "npm install react-router && npx flowbite-react@latest init && npm install react-router-dom && npm install formik && npm i yup &&  npm i swiper && npm i react-icons && npm i react-infinite-scroll-component && npm i axios && npm install dotenv && npm install firebase && npm install react-toastify && npm install scrollreveal && npm i prop-types && npm install react-share"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.17",
    "axios": "^1.9.0",
    "dotenv": "^16.5.0",
    "firebase": "^11.6.1",
    "flowbite-react": "^0.11.7",
    "formik": "^2.4.6",
    "i": "^0.3.7",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-router": "^7.5.2",
    "react-router-dom": "^7.5.2",
    "react-share": "^5.2.2",
    "react-toastify": "^11.0.5",
    "scrollreveal": "^4.0.9",
    "swiper": "^11.2.6",
    "tailwind-scrollbar-hide": "^2.0.0",
    "tailwindcss": "^4.0.17",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "vite": "^6.2.0"
  }
}
